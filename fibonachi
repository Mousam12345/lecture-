import java.util.*;

public class fibonachi {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = 10  , a =0 , b = 1;
        System.out.println("fibonachi series :"+ a + ", " + b);

        for(int i = 2 ; i < num ; i++ ){
            int next = ( a + b);
            System.out.println(" " + next);
            a = b;
            b = next ; 

        }
        
    }
}
